{
  "name": "socket-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/socket-service/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/socket-service",
        "generatePackageJson": true,
        "main": "apps/socket-service/src/main.ts",
        "tsConfig": "apps/socket-service/tsconfig.app.json",
        "assets": ["apps/socket-service/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/socket-service/src/environments/environment.ts",
              "with": "apps/socket-service/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "socket-service:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "socket-service:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/socket-service/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/socket-service/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "options": {
        "metadata": {
          "images": ["ghcr.io/IFS49F/poker/socket-service"],
          "load": true,
          "tags": [
            "type=schedule",
            "type=ref,event=branch",
            "type=ref,event=tag",
            "type=ref,event=pr",
            "type=semver,pattern={{version}}",
            "type=semver,pattern={{major}}.{{minor}}",
            "type=semver,pattern={{major}}",
            "type=sha"
          ],
          "labels": [
            "org.opencontainers.image.source=https://github.com/IFS49F/poker",
            "org.opencontainers.image.description=The socket service of Poker4Fun",
            "org.opencontainers.image.licenses=GPL-3.0"
          ]
        }
      }
    }
  },
  "tags": []
}
